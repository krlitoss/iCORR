	<queryString>
		<![CDATA[SELECT entradas_productos.fecha,proveedores.nombre,entradas_productos_detalle.*,productos.ancho,productos.peso,productos.descripcion,productos.unidad,COALESCE(consumos.cantidadconsumo,0) as cantidadconsumo,consumos.fechaconsumo,(entradas_productos_detalle.CANTIDAD-COALESCE(consumos.cantidadconsumo,0)) as inventario FROM (entradas_productos LEFT JOIN proveedores ON entradas_productos.id_proveedor=proveedores.id_proveedor) INNER JOIN ((entradas_productos_detalle LEFT JOIN (SELECT consumos_productos.id_entrada_producto_detalle,sum(consumos_productos.cantidad) as cantidadconsumo,max(consumos_productos.fecha) as fechaconsumo FROM consumos_productos WHERE (consumos_productos.estatus<>'Can' AND consumos_productos.fecha<=$P{fechaini}) GROUP BY consumos_productos.id_entrada_producto_detalle) as consumos ON entradas_productos_detalle.id_entrada_producto_detalle=consumos.id_entrada_producto_detalle) LEFT JOIN productos ON entradas_productos_detalle.clave_producto=productos.clave) ON entradas_productos.id_entrada_producto=entradas_productos_detalle.id_entrada_producto WHERE (entradas_productos_detalle.estatus<>'Can' AND entradas_productos.fecha<=$P{fechaini} AND (entradas_productos_detalle.CANTIDAD-COALESCE(consumos.cantidadconsumo,0))>0) ORDER BY entradas_productos_detalle.almacen_existe,entradas_productos.fecha,entradas_productos_detalle.id_entrada_producto_detalle;]]>
