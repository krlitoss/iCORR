	<queryString>
		<![CDATA[SELECT
  ordenescompra.id_ordencompra,
  ordenescompra.fecha,
  ordenescompra.fechaentrega,
  ordenescompra.status,
  proveedores.nombre,
  ordenescompra.id_proveedor,
  ordenescompra.total,
  ordenescompra.recepcion,
  monedas.descripcion
FROM proveedores RIGHT JOIN (
  ordenescompra LEFT JOIN monedas ON ordenescompra.id_moneda=monedas.id_moneda)
  ON proveedores.id_proveedor = ordenescompra.id_proveedor
WHERE (((ordenescompra.fecha)>=$P{fechaini} AND (ordenescompra.fecha)<=$P{fechafin}) AND ordenescompra.status<>'Cancelado' $P!{senSQL})
  ORDER BY proveedores.nombre,  monedas.descripcion,ordenescompra.id_ordencompra;]]>
